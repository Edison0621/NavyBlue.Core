{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServiceRegisterEntity": {
    "ServiceName": "NavyBlue.Demo.ServiceGovern",
    "ServicePort": "4877",
    "ConsulServer": "http://127.0.0.1:8500/",
    "IsHttps": false,
    "IsIPV6": false,
    "HealthCheck": {
      "DeregisterCriticalServiceAfter": 5,
      "Interval": 10,
      "HTTP": "api/health",
      "Timeout": 10
    }
  }
}